{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "customers-api",
      "image": "jaimayal/customers-api:06.05.2023.00.55.42",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 8081,
          "containerPort": 8081
        }
      ],
      "environment": [
        {
          "name": "SPRING_DATASOURCE_URL",
          "value": "jdbc:postgresql://TODO:5432/customer"
        }
      ]
    },
    {
      "name": "customers-frontend",
      "image": "jaimayal/customers-frontend:latest",
      "essential": true,
      "memory": 256,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 5173
        }
      ]
    }
  ]
}